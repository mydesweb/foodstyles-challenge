{
  "name": "rn-app",
  "version": "1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "postinstall": "npx typesync && npx patch-package && npx pod-install",
    "generate:bootsplash": "react-native generate-bootsplash src/assets/logo.svg --platforms=android,ios --background=#FFFFFF --dark-background=#FFFFFF --logo-width=160",
    "ios:update-pods": "cd ios && pod install --repo-update && cd ..",
    "link-assets:ios": "npx react-native-asset -ios-a src/assets/fonts",
    "link-assets:android": "npx react-native-asset -android-a src/assets/fonts"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.3",
    "@react-native-segmented-control/segmented-control": "^2.5.2",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.10.0",
    "@vonovak/react-native-theme-control": "^6.0.0",
    "expo": "^51.0.0",
    "expo-image": "~1.12.12",
    "expo-linear-gradient": "~13.0.2",
    "immer": "^10.1.1",
    "moti": "^0.29.0",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-bootsplash": "^5.5.3",
    "react-native-gesture-handler": "^2.17.1",
    "react-native-reanimated": "^3.12.1",
    "react-native-safe-area-context": "^4.10.7",
    "react-native-screens": "^3.32.0",
    "react-native-vector-icons": "^10.1.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.74.85",
    "@react-native/eslint-config": "0.74.85",
    "@react-native/metro-config": "0.74.85",
    "@react-native/typescript-config": "0.74.85",
    "@types/babel__core": "~7.20.5",
    "@types/babel__preset-env": "~7.9.7",
    "@types/eslint": "~8.56.10",
    "@types/prettier": "~3.0.0",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "~6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.3.1"
}
